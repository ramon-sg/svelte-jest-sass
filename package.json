{
  "name": "TODO",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss src/style/tailwind-setup.css -o src/style/tailwind.css -w",
    "build:tailwind": "NODE_ENV=production postcss src/style/tailwind-setup.css -o src/style/tailwind.css",
    "build": "npm run build:tailwind && rollup -c",
    "dev": "NODE_ENV=dev rollup -c -w",
    "start": "sirv public",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll",
    "serve": "run-p dev watch:tailwind"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^26.0.1",
    "custom-env": "^2.0.1",
    "jest": "^26.0.1",
    "jest-transform-svelte": "^2.1.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.9.6",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  }
}
